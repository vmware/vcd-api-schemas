<xs:schema xmlns="http://www.vmware.com/vcloud/v1.5"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
           xmlns:meta="http://www.vmware.com/vcloud/meta"
           jaxb:version="2.0"
           jaxb:extensionBindingPrefixes="meta"
           elementFormDefault="qualified"
           targetNamespace="http://www.vmware.com/vcloud/v1.5"
           version="1.0">

    <xs:include schemaLocation="task.xsd"/>

    <xs:annotation>
        <xs:documentation xml:lang="en">
            Defines types common to most or all of the vCloud
            request/response schemas.
        </xs:documentation>
    </xs:annotation>

    <xs:complexType name="VCloudExtensionType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                General purpose extension element. Not related to extension
                services.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
        </xs:sequence>

        <xs:attribute name="required" type="xs:boolean" default="true" use="optional">
            <xs:annotation>
                <xs:documentation source="modifiable">always</xs:documentation>
                <xs:documentation xml:lang="en">
                    True if the request should fail when the server does not
                    understand the extension.
                </xs:documentation>
                <xs:documentation source="required">false</xs:documentation>
            </xs:annotation>
        </xs:attribute>

        <xs:anyAttribute namespace="##other" processContents="lax"/>
    </xs:complexType>

    <xs:complexType abstract="true" name="VCloudExtensibleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                A base abstract type for all complex types that support extensions.
            </xs:documentation>
        </xs:annotation>

        <xs:sequence>
            <xs:element name="VCloudExtension" type="VCloudExtensionType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        An optional extension element that can contain an
                        arbitrary number of elements and attributes. Not
                        related to extension services.
                    </xs:documentation>
                    <xs:documentation source="required">false</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>

        <xs:anyAttribute namespace="##other" processContents="lax"/>
    </xs:complexType>

    <xs:attributeGroup name="CommonErrorAttributes">
        <xs:attribute name="message" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation source="modifiable">
                    none
                </xs:documentation>
                <xs:documentation xml:lang="en">
                    An one line, human-readable message describing the
                    error that occurred.
                </xs:documentation>
                <xs:documentation source="required">
                    true
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="majorErrorCode" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation source="modifiable">
                    none
                </xs:documentation>
                <xs:documentation xml:lang="en">
                    The class of the error. Matches the HTTP status
                    code.
                </xs:documentation>
                <xs:documentation source="required">
                    true
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="minorErrorCode" type="xs:string" use="required">
            <xs:annotation>
                <xs:documentation source="modifiable">
                    none
                </xs:documentation>
                <xs:documentation xml:lang="en">
                    Resource-specific error code.
                </xs:documentation>
                <xs:documentation source="required">
                    true
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="vendorSpecificErrorCode" type="xs:string"
            use="optional">
            <xs:annotation>
                <xs:documentation source="modifiable">
                    none
                </xs:documentation>
                <xs:documentation xml:lang="en">
                    A vendor- or implementation-specific error code that
                    can reference specific modules or source lines for
                    diagnostic purposes.
                </xs:documentation>
                <xs:documentation source="required">
                    false
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>

    <xs:element name="Error" type="ErrorType"/>
    <xs:complexType name="ErrorType">
        <xs:annotation>
            <xs:appinfo>
                <meta:content-type>application/vnd.vmware.vcloud.error</meta:content-type>
            </xs:appinfo>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                The standard error message type used in the vCloud REST API.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="TenantError" type="TenantErrorType" maxOccurs="1" minOccurs="0">
                        <xs:annotation>
                            <xs:appinfo><meta:version added-in="5.7"/></xs:appinfo>
                            <xs:documentation xml:lang="en">
                                The tenant view of the error, when the containing error is an
                                administrator view of the error.
                            </xs:documentation>
                            <xs:documentation source="since">5.7</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>                    
                    </xs:element>
                </xs:sequence>
                <xs:attributeGroup ref="CommonErrorAttributes"/>
                <xs:attribute name="stackTrace" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="since">1.0</xs:documentation>
                        <xs:documentation source="modifiable">
                            none
                        </xs:documentation>
                        <xs:documentation xml:lang="en">
                            The stack trace of the exception.
                        </xs:documentation>
                        <xs:documentation source="required">
                            false
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="TenantErrorType">
        <xs:annotation>
            <xs:appinfo><meta:version added-in="5.7"/></xs:appinfo>
            <xs:documentation source="since">5.7</xs:documentation>
            <xs:documentation xml:lang="en">
                The tenant view of the error, when the containing error is an
                administrator view of the error.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:attributeGroup ref="CommonErrorAttributes" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ResultType">
        <xs:annotation>
            <xs:appinfo><meta:version added-in="13.0"/></xs:appinfo>
            <xs:documentation source="since">13.0</xs:documentation>
            <xs:documentation xml:lang="en">
                The result of operations.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ResultContent" type="xs:anyType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo><meta:version added-in="13.0"/></xs:appinfo>
                    <xs:documentation source="since">13.0</xs:documentation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        An optional element that can be used to hold the result
                        of operations.
                    </xs:documentation>
                    <xs:documentation source="required">false</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ResultReference" type="ReferenceType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:appinfo><meta:version added-in="33.0"/></xs:appinfo>
                    <xs:documentation source="since">33.0</xs:documentation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation>
                        An optional element that can be used to hold the reference
                        of a result.
                    </xs:documentation>
                    <xs:documentation source="required">false</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="Link" type="LinkType"/>
    <xs:complexType name="LinkType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Extends reference type by adding relation attribute.
                Defines a hyper-link with a relationship, hyper-link reference,
                and an optional MIME type.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="ReferenceType">
                <xs:attribute name="rel" type="xs:string" use="required">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Defines the relationship of the link to the object
                            that contains it. A relationship can be the name of
                            an operation on the object, a reference to a
                            contained or containing object, or a reference to
                            an alternate representation of the object. The
                            relationship value implies the HTTP verb to use
                            when you use the link's href as a request URL. See
                            the vCloud API Programming Guide for a list of
                            links and link relations.
                        </xs:documentation>
                        <xs:documentation source="required">true</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="model" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:appinfo>
                            <meta:version added-in="33.0"></meta:version>
                        </xs:appinfo>
                        <xs:documentation source="since">33.0</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Defines the model expected for modification calls. This
                            field is populated when the href is expected to link to
                            a vCloud OpenApi endpoint.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ResourceType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                The base type for all objects in the vCloud model.
                Has an optional list of links and href and type attributes.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="Link" type="LinkType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A reference to an entity or operation associated
                                with this object.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="href" type="xs:anyURI" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The URI of the entity.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="type" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The MIME type of the entity.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="IdentifiableResourceType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                The base type for all resource types which contain an
                id attribute.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="ResourceType">
                <xs:attribute name="id" type="xs:string">
                    <xs:annotation>
                        <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The entity identifier, expressed in URN format.
                            The value of this attribute uniquely identifies
                            the entity, persists for the life of the entity,
                            and is never reused.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="operationKey" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:appinfo><meta:version added-in="5.1"/></xs:appinfo>
                        <xs:documentation source="modifiable">create</xs:documentation>
                        <xs:documentation xml:lang="en">
                           Optional unique identifier to support idempotent
                           semantics for create and delete operations.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ParamsType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                A basic type used to specify request parameters.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="Description" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Optional description.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="name" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Typically used to name or identify the subject of
                            the request. For example, the name of the object
                            being created or modified.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ReferenceType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                A reference to a resource. Contains an href attribute and
                optional name and type attributes.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:attribute name="href" type="xs:anyURI" use="required">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Contains the URI to the entity.
                        </xs:documentation>
                        <xs:documentation source="required">true</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="id" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The resource identifier, expressed in URN format.
                            The value of this attribute uniquely identifies
                            the resource, persists for the life of the resource,
                            and is never reused.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="type" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Contains the type of the the entity.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="name" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Contains the name of the the entity.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ResourceReferenceType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a reference to a resource. Contains an href
                attribute, a resource status attribute, and optional name and
                type attributes.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="ReferenceType">
                <xs:attribute name="status" type="xs:int" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Status of a resource.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ContainerType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
               Container for query result sets.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ResourceType">
                <xs:attribute name="name" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Query name that generated this result set.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="page" type="xs:int" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Page of the result set that this container holds.
                            The first page is page number 1.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="pageSize" type="xs:int" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Page size, as a number of records or references.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="total" type="xs:long" use="optional">
                    <xs:annotation>
                       <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Total number of records or references in the
                            container.
                        </xs:documentation>
                        <xs:documentation source="required">false</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CapacityType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents the capacity of a given resource.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="Units" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Units in which capacity is allocated. For CPU capacity, one of:&lt;br&gt;
                                MHz&lt;br&gt;
                                GHz&lt;br&gt;
                                For memory capacity, one of:&lt;br&gt;
                                MB&lt;br&gt;
                                GB
                            </xs:documentation>
                            <xs:documentation source="required">true</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Allocated" type="xs:long" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Capacity that is committed to be available.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Limit" type="xs:long" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Capacity limit relative to the value specified for Allocation.
                                It must not be less than that value. If it is greater than that value,
                                it implies overprovisioning.
                            </xs:documentation>
                            <xs:documentation source="required">true</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CapacityWithUsageType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a capacity and usage of a given resource.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="CapacityType">
                <xs:sequence>
                    <xs:element name="Reserved" type="xs:long" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">5.1</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Capacity reserved.
                            </xs:documentation>
                            <xs:appinfo>
                                <meta:version added-in="5.1"/>
                            </xs:appinfo>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Used" type="xs:long" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Capacity used. If the VDC AllocationModel is 
                                ReservationPool, this number represents the 
                                percentage of the reservation that is in use.
                                For all other allocation models, it represents
                                the percentage of the allocation that is in
                                use.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Overhead" type="xs:long" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:appinfo><meta:version removed-in="REMOVED_VERSION_15_51"/></xs:appinfo>
                            <xs:documentation xml:lang="en">
                                Number of Units allocated to system resources
                                such as vShield Manager virtual machines and
                                shadow virtual machines provisioned from this
                                Provider vDC.
                            </xs:documentation>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ReservationUsed" type="xs:long" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">35.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Reservation Capacity used. This field is returned
                                only if the VDC AllocationModel is
                                Flex type. This number represents the
                                reservation resources used in the given VDC.
                            </xs:documentation>
                            <xs:appinfo>
                                <meta:version added-in="35.0"/>
                            </xs:appinfo>
                            <xs:documentation source="required">false</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="AccessSettingType">
        <xs:annotation xml:lang="en">
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Controls access to the resource.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:choice minOccurs="1" maxOccurs="1">
                        <xs:element name="Subject" type="ReferenceType">
                            <xs:annotation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    The user or group to which these settings apply.
                                </xs:documentation>
